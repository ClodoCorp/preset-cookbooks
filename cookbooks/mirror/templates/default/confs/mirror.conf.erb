
#proxy_cache_lock on;
#proxy_cache_lock_timeout 5s;
proxy_cache_min_uses 2;

proxy_cache_path /var/www/cache levels=1:2 keys_zone=cache:10m inactive=25m max_size=2000M;
proxy_temp_path /var/www/temp 1 2;

proxy_cache cache;
proxy_cache_bypass $http_pragma    $http_authorization;


proxy_cache_use_stale error;# | timeout | invalid_header | updating | http_500 | http_502 | http_503 | http_504 | http_404

#proxy_cache_valid 200 304 12h;
#proxy_cache_valid 302 301 12h;
#proxy_cache_valid any 1m;
proxy_cache_valid 6h;

#proxy_hide_header ???;
proxy_ignore_client_abort off;
proxy_ignore_headers Cache-Control Expires;
proxy_pass_header Set-Cookie;
proxy_intercept_errors on;
proxy_no_cache $http_pragma    $http_authorization;

proxy_connect_timeout 10s;
proxy_read_timeout 10s;
proxy_send_timeout 10s;


